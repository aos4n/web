    "test": "jest --silent --no-watchman"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aos4n/web.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "web"
  ],
  "author": "aos4n",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aos4n/web/issues"
  },
  "homepage": "https://github.com/aos4n/web#readme",
  "dependencies": {
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-router": "^8.0.8",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/koa": "^2.11.3",
    "@types/koa-router": "^7.4.0",
    "@types/koa-static": "^4.0.1",
    "@types/koa2-cors": "^2.0.1",
    "@types/node": "^13.13.4",
    "@types/supertest": "^2.0.8",
    "jest": "^25.5.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
